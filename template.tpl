___SANDBOXED_JS_FOR_WEB_TEMPLATE___

// Required APIs
const callInWindow = require('callInWindow');
const makeString = require('makeString');
const logToConsole = require('logToConsole');

// Retrieve user inputs
const eventName = makeString(data.aHREFTrackingEvent);
const loggingDisabled = data.disableConsoleLogging === true;

// Trigger the event
callInWindow('AhrefsAnalytics.sendEvent', eventName);

// Logging if enabled
if (!loggingDisabled) {
  logToConsole('🟢 Ahrefs event sent: ' + eventName + ' [Event Tracking Only]');
}

// Signal success to GTM
data.gtmOnSuccess();


